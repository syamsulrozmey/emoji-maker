# Replicate API Token
# Get your API token from https://replicate.com/account/api-tokens
REPLICATE_API_TOKEN=r8_your_api_token_here

# Supabase Configuration
# Get these from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Clerk Authentication
# Get these from your Clerk dashboard
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here
CLERK_SECRET_KEY=your_clerk_secret_key_here

# Stripe Payment Integration
# Get these from your Stripe dashboard (https://dashboard.stripe.com)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Stripe Price IDs (create products in Stripe Dashboard first)
STRIPE_STARTER_PACK_PRICE_ID=price_your_starter_pack_price_id
STRIPE_PRO_PACK_PRICE_ID=price_your_pro_pack_price_id
STRIPE_PRO_MONTHLY_PRICE_ID=price_your_pro_monthly_price_id

# Application URL (for Stripe redirects)
NEXT_PUBLIC_APP_URL=http://localhost:3000

